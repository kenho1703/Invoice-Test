    <script src="Client/app.js"></script>
    <script src="Client/config.router.js"></script>

    <script src="Client/services/ui-load.js"></script>


    <script src="Client/services/product.service.js"></script>
    <script src="Client/services/invoice.service.js"></script>

    <script src="Client/invoice/invoice.controller.js"></script>
    <script src="Client/dashboard/dashboard.controller.js"></script>
    <script src="Client/directives/ui-nav.js"></script>
